{
  "name": "Jchat",
  "version": "1.0.0",
  "description": "A simple chat working with sockets :)",
  "main": "lib/app.js",
  "author": "Jay Roslindo Donguines <creeped12@gmail.com>",
  "scripts": {
    "start": "node ./lib/app.js",
    "compile": "babel -d lib/ src/",
    "run": "babel -d lib/ src/ && node lib/app",
    "serve": "npm run compile && npm start"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "license": "MIT",
  "repository": "Psalms23/socket_chat",
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "http": "0.0.0",
    "jQuery": "^1.7.4",
    "moment": "^2.19.3",
    "mongodb": "^2.2.33",
    "path": "^0.12.7",
    "socket.io": "^1.4.8"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "istanbul": "^0.4.3"
  }
}
